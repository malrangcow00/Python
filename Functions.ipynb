    "def buy_item(cost_of_item):\n",
    "    return cost_of_item + add_tax_to_item(cost_of_item)\n",
    "\n",
    "\n",
    "def add_tax_to_item(cost_of_item):\n",
    "    current_tax_rate = .03\n",
    "    return cost_of_item * current_tax_rate\n",
    "\n",
    "\n",
    "final_cost = buy_item(50)\n",
    "print(final_cost)"
